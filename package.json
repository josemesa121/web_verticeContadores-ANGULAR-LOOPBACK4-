{
  "name": "vertice",
  "version": "1.0.0",
  "description": "tareas",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t vertice .",
    "docker:run": "docker run -p 3000:3000 -d vertice",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start-original": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "start": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js --env production"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^3.0.0",
    "@loopback/boot": "1.5.8",
    "@loopback/context": "1.22.1",
    "@loopback/core": "1.10.4",
    "@loopback/openapi-v3": "1.9.6",
    "@loopback/repository": "1.15.1",
    "@loopback/rest": "1.20.1",
    "@loopback/rest-explorer": "1.4.1",
    "@loopback/service-proxy": "1.3.8",
    "@types/jsonwebtoken": "8.3.3",
    "@types/multer": "^1.3.10",
    "bcryptjs": "2.4.3",
    "cross-blob": "^1.2.2",
    "debug": "4.1.1",
    "es-abstract": "^1.14.2",
    "excel4node": "^1.7.2",
    "express": "4.17.1",
    "isemail": "3.2.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "loopback-connector-kv-redis": "3.0.1",
    "loopback-connector-mongodb": "5.0.0",
    "loopback-connector-mysql": "^5.4.2",
    "loopback-connector-rest": "3.4.2",
    "moment": "^2.29.4",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "stream-buffers": "^3.0.2",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@loopback/build": "2.0.10",
    "@loopback/testlab": "1.8.0",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.1",
    "@types/lodash": "4.14.138",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.5",
    "mocha": "6.2.0",
    "source-map-support": "0.5.13",
    "typescript": "3.6.3"
  }
}
